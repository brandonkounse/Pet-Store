<%= render 'orders/store_link' %>

<h1>List of Pets</h1>

<ul class="flex">
    <li><%= link_to "Add Pet", new_pet_path %></li>
    <li>
        <%= form_with url: '/search', method: :get do |form| %>
            <%= form.label :search, 'Search for pet' %>
            <%= form.text_field :search %>
            <%= form.submit 'Search' %>
        <% end %>
    </li>
</ul>

<div><%= render 'pet_table' %></div>
